package oop1;

public class Product {
	private String name;
	private double unitPrice;
	private double discount; //indirim oranı
	private String imageUrl;
	private int unitsInStock; // private yapınca sadece bu class da kullanılabilir oluyor.
	// sağ tıkla sourca > generate setters and .. olanını seçip verileri dışa açabilirsin.
	
	//set yazmak için, get okumak için
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public double getUnitPrice() {
		return unitPrice;
	}
	public void setUnitPrice(double unitPrice) {
		this.unitPrice = unitPrice;
	}
	public double getDiscount() {
		return discount;
	}
	//public void setDiscount(double discount) {
		//this.discount = discount;
		//alınan yeni kararla birlikte discount verisi yazılamsın derseler bu blok silinir.
	//}
	public String getImageUrl() {
		return imageUrl;
	}
	public void setImageUrl(String imageUrl) {
		this.imageUrl = imageUrl;
	}
	public int getUnitsInStock() {
		return unitsInStock;
	}
	public void setUnitsInStock(int unitsInStock) {
		this.unitsInStock = unitsInStock;
	}
	
}
